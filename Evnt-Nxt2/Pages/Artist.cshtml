@page
@model Evnt_Nxt2.Pages.ArtistModel;

<h1>Artist List</h1>


<ul>
    @foreach (var Artist in Model.ArtistList)
    {
        <li>
            <a asp-page="/Events/Details" asp-route-id="@Artist.ID">@Artist.Name</a>
            @foreach (var genre in Artist.Genres)
            {
                <a asp-page="/Genres" asp-route-id="@genre">@genre</a>
            }
        </li>
    }
</ul>
